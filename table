import java.util.Scanner;
public class table {
    public static void main(String[] args) {
        Scanner st = new Scanner(System.in);
        System.out.println("Enter your number=");
        int num= st.nextInt();
        for(int i=1;i<=10;i++)
        {
        System.out.println(num+"*"+i+"="+num*i);
        }
    }
}
